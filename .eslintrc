{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "globalReturn": true
    }    
  },
  "env" : {
    "node" : true,
    "es6": true,
    "browser" : false,
    "mocha" : true
  },
  "rules": {
    "no-console": 0,
    "semi" : 0, 
    "camelcase" : 0, 
    "no-multi-spaces" : 0, 
    "max-len" : 0, 
    "prefer-const": 0
  }
}
